
aws codeartifact login --tool pip --repository $REPO_NAME --domain $DOMAIN_NAME --domain-owner $ACCOUNT_ID --region $REGION

pip config set global.extra-index-url https://pypi.org/simple

poetry config repositories.codebase `aws codeartifact get-repository-endpoint --domain $DOMAIN_NAME --domain-owner $ACCOUNT_ID --repository $REPO_NAME --region $REGION --format pypi --query repositoryEndpoint --output text`

poetry config http-basic.codebase aws `aws codeartifact get-authorization-token --domain $DOMAIN_NAME --domain-owner $ACCOUNT_ID --region $REGION --query authorizationToken --output text`
